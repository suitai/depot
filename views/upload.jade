.container
  form.form-horizontal(method="POST", action="/upload", enctype="multipart/form-data")
    .form-group
      label.control-label Tag
      .col-md-10
        input(type="text", name="tag")
    .form-group
      label.control-label File
      .col-md-10
        input(type="file", name="file")
    button(type="submit") Upload
